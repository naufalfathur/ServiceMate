<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/b"
    tools:context=".ui.home.HomeFragment">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/b">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="@color/b">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="600sp"
                android:layout_marginTop="100dp"
                android:background="@drawable/bg_home"
                android:orientation="vertical">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="50dp"
                    android:layout_marginLeft="19dp"
                    android:layout_marginRight="19dp"
                    android:text="About"
                    android:textColor="#000"
                    android:textAlignment="center"
                    android:textSize="22dp"
                    android:textStyle="italic"
                    tools:ignore="DuplicateIds"
                    android:gravity="center_horizontal" />
                <ImageView
                    android:id="@+id/imageView"
                    android:layout_width="match_parent"
                    android:layout_marginTop="40dp"
                    android:layout_height="100dp"
                    android:src="@drawable/sm"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:layout_editor_absoluteX="0dp"
                    tools:ignore="MissingConstraints" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="19dp"
                    android:textAlignment="center"
                    android:text="ServiceMate"
                    android:layout_marginRight="19dp"
                    android:textColor="@color/b"
                    android:textSize="22dp"
                    android:textStyle="italic"
                    tools:ignore="DuplicateIds"
                    android:gravity="center_horizontal" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:layout_marginLeft="19dp"
                    android:layout_marginRight="19dp"
                    android:text="MOBILE APP SKR4307 (G3) PROJECT"
                    android:textColor="@color/Dblue"
                    android:textAlignment="center"
                    android:textSize="15dp"
                    android:textStyle="bold"
                    tools:ignore="DuplicateIds"
                    android:gravity="center_horizontal" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="19dp"
                    android:layout_marginRight="19dp"
                    android:text="Created By"
                    android:textColor="@color/Dblue"
                    android:textAlignment="center"
                    android:textSize="15dp"
                    android:textStyle="bold"
                    tools:ignore="DuplicateIds"
                    android:gravity="center_horizontal" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="19dp"
                    android:layout_marginRight="19dp"
                    android:textAlignment="center"
                    android:text="Naufal Fathur Rahman Wahid 195085‬"
                    android:textColor="@color/Dblue"
                    android:textSize="15dp"
                    android:textStyle="italic"
                    tools:ignore="DuplicateIds"
                    android:gravity="center_horizontal" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="19dp"
                    android:layout_marginRight="19dp"
                    android:textAlignment="center"
                    android:text="Farhan Ahmed 191522‬"
                    android:textColor="@color/Dblue"
                    android:textSize="15dp"
                    android:textStyle="italic"
                    tools:ignore="DuplicateIds"
                    android:gravity="center_horizontal" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="19dp"
                    android:layout_marginRight="19dp"
                    android:textAlignment="center"
                    android:text="OSAMAH EBRAHIM ‬"
                    android:textColor="@color/Dblue"
                    android:textSize="15dp"
                    android:textStyle="italic"
                    tools:ignore="DuplicateIds"
                    android:gravity="center_horizontal" />
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="19dp"
                    android:layout_marginRight="19dp"
                    android:textAlignment="center"
                    android:text="CHEN WEIYOU‬"
                    android:textColor="@color/Dblue"
                    android:textSize="15dp"
                    android:textStyle="italic"
                    tools:ignore="DuplicateIds"
                    android:gravity="center_horizontal" />



            </LinearLayout>
        </LinearLayout>
    </ScrollView>
    <LinearLayout
        android:id="@+id/paid"
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:layout_marginTop="50dp"
        android:background="@color/b"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent">

        <TextView
            android:id="@+id/texasxzscastView3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:layout_marginLeft="19dp"
            android:layout_marginRight="19dp"
            android:text="Back"
            android:textColor="#FFF"
            android:textAlignment="center"
            android:textSize="22dp"
            android:textStyle="italic"
            tools:ignore="DuplicateIds"
            android:gravity="center_horizontal" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>