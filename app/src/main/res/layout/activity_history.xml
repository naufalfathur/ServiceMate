<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/b"
    tools:context=".ui.home.HomeFragment">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="700dp"
        android:background="@drawable/bg_home"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent">

        <TextView
            android:id="@+id/textViasxcdaew3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="19dp"
            android:gravity="center_horizontal"
            android:text="Booking History"
            android:textAlignment="center"
            android:textColor="#000"
            android:textSize="22dp"
            android:textStyle="bold"
            tools:ignore="DuplicateIds" />
        <LinearLayout
            android:id="@+id/first"
            android:layout_width="match_parent"
            android:layout_height="280dp"
            android:layout_marginLeft="9dp"
            android:layout_marginTop="39dp"
            android:layout_marginRight="9dp"
            android:background="@drawable/list"
            android:baselineAligned="false"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="19dp"
                android:textAlignment="center"
                android:text="Active"
                android:layout_marginRight="19dp"
                android:textColor="@color/b"
                android:textSize="15dp"
                android:textStyle="italic"
                tools:ignore="DuplicateIds"
                android:gravity="center_horizontal" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:textAlignment="center"
                android:layout_marginTop="10dp"
                android:text="Suasa Suria Auto Sdn. Bhd."
                android:textColor="#000"
                android:textStyle="italic"
                android:textSize="15dp"
                tools:ignore="DuplicateIds"
                android:gravity="center_horizontal" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAlignment="center"
                android:text="No.13, Jalan SR 4/19,Taman Serdang Raya ..."
                android:textColor="#000"
                android:textSize="13dp"
                tools:ignore="DuplicateIds"
                android:gravity="center_horizontal" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAlignment="center"
                android:text="Tel: 0389450801"
                android:textColor="#000"
                android:textSize="13dp"
                android:textStyle="italic"
                tools:ignore="DuplicateIds"
                android:gravity="center_horizontal" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="19dp"
                android:textAlignment="center"
                android:text="Maintenance Service"
                android:layout_marginRight="19dp"
                android:textColor="@color/b"
                android:textSize="22dp"
                android:textStyle="italic"
                tools:ignore="DuplicateIds"
                android:gravity="center_horizontal" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="19dp"
                android:layout_marginRight="19dp"
                android:text="Wednesday, 22 July 2020 on 3:00"
                android:textColor="#000"
                android:textAlignment="center"
                android:textSize="15dp"
                android:textStyle="bold"
                tools:ignore="DuplicateIds"
                android:gravity="center_horizontal" />

            <Button
                android:id="@+id/cancelb"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginLeft="44dp"
                android:layout_marginTop="16dp"
                android:layout_marginRight="44dp"
                android:background="@color/yellow"
                android:onClick="cancel"
                android:text="Cancel Book"
                android:textColor="#FFF"
                tools:ignore="OnClick" />
            <Button
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginLeft="44dp"
                android:layout_marginTop="16dp"
                android:layout_marginRight="44dp"
                android:background="@color/b"
                android:onClick="review"
                android:text="Leave Review"
                android:textColor="#FFF"
                tools:ignore="OnClick" />

        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="230dp"
            android:layout_marginLeft="9dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="9dp"
            android:background="@drawable/list"
            android:baselineAligned="false"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="19dp"
                android:textAlignment="center"
                android:text="Expired"
                android:layout_marginRight="19dp"
                android:textColor="#FF725E"
                android:textSize="15dp"
                android:textStyle="italic"
                tools:ignore="DuplicateIds"
                android:gravity="center_horizontal" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:textAlignment="center"
                android:layout_marginTop="30dp"
                android:text="DMM Sales Sdn. Bhd."
                android:textColor="#000"
                android:textStyle="italic"
                android:textSize="15dp"
                tools:ignore="DuplicateIds"
                android:gravity="center_horizontal" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="27dp"
                android:textAlignment="center"
                android:layout_marginTop="60dp"
                android:layout_marginBottom="29dp"
                android:text="No. 3, Lorong 7/3A,Light Industrial Area, ..."
                android:textColor="#000"
                android:textSize="13dp"
                tools:ignore="DuplicateIds"
                android:gravity="center_horizontal" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="27dp"
                android:textAlignment="center"
                android:layout_marginTop="80dp"
                android:text="Tel: 0389450801"
                android:textColor="#000"
                android:textSize="13dp"
                android:textStyle="italic"
                tools:ignore="DuplicateIds"
                android:gravity="center_horizontal" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="100dp"
                android:layout_marginLeft="19dp"
                android:textAlignment="center"
                android:text="Maintenance Service"
                android:layout_marginRight="19dp"
                android:textColor="@color/b"
                android:textSize="22dp"
                android:textStyle="italic"
                tools:ignore="DuplicateIds"
                android:gravity="center_horizontal" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="140dp"
                android:layout_marginLeft="19dp"
                android:layout_marginRight="19dp"
                android:text="Wednesday, 20 July 2020 on 8:00"
                android:textColor="#000"
                android:textAlignment="center"
                android:textSize="15dp"
                android:textStyle="bold"
                tools:ignore="DuplicateIds"
                android:gravity="center_horizontal" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginLeft="44dp"
                android:layout_marginTop="180dp"
                android:layout_marginRight="44dp"
                android:background="@color/b"
                android:onClick="review"
                android:text="Leave Review"
                android:textColor="#FFF"
                tools:ignore="OnClick" />

        </RelativeLayout>



    </LinearLayout>
    <LinearLayout
        android:id="@+id/paid"
        android:layout_width="match_parent"
        android:layout_height="73dp"
        android:layout_marginTop="5dp"
        android:background="@color/b"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="19dp"
            android:layout_marginRight="19dp"
            android:text="Back to Home"
            android:textColor="#FFF"
            android:textAlignment="center"
            android:textSize="22dp"
            android:textStyle="italic"
            tools:ignore="DuplicateIds"
            android:gravity="center_horizontal" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>