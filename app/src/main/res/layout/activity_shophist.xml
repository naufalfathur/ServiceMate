<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/b"
    tools:context=".ui.home.HomeFragment">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="700dp"
        android:background="@drawable/bg_home"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent">

        <TextView
            android:id="@+id/textViasxcdaew3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="19dp"
            android:gravity="center_horizontal"
            android:text="Shop History"
            android:textAlignment="center"
            android:textColor="#000"
            android:textSize="22dp"
            android:textStyle="bold"
            tools:ignore="DuplicateIds" />
        <LinearLayout
            android:id="@+id/first"
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:layout_marginLeft="9dp"
            android:layout_marginTop="39dp"
            android:layout_marginRight="9dp"
            android:background="@drawable/list"
            android:baselineAligned="false"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="19dp"
                android:textAlignment="center"
                android:text="Payment Confirmed"
                android:layout_marginRight="19dp"
                android:textColor="@color/b"
                android:textSize="15dp"
                android:textStyle="italic"
                tools:ignore="DuplicateIds"
                android:gravity="center_horizontal" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="19dp"
                android:textAlignment="center"
                android:text="Shop Parts"
                android:layout_marginRight="19dp"
                android:textColor="@color/b"
                android:textSize="22dp"
                android:textStyle="italic"
                tools:ignore="DuplicateIds"
                android:gravity="center_horizontal" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="19dp"
                android:layout_marginRight="19dp"
                android:text="BridgeStone Tires"
                android:textColor="#000"
                android:textAlignment="center"
                android:textSize="15dp"
                android:textStyle="bold"
                tools:ignore="DuplicateIds"
                android:gravity="center_horizontal" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="19dp"
                android:layout_marginRight="19dp"
                android:layout_marginTop="10dp"
                android:text="Accu Optima"
                android:textColor="#000"
                android:textAlignment="center"
                android:textSize="15dp"
                android:textStyle="bold"
                tools:ignore="DuplicateIds"
                android:gravity="center_horizontal" />
            <Button
                android:id="@+id/cancel"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_marginLeft="44dp"
                android:layout_marginTop="16dp"
                android:layout_marginRight="44dp"
                android:background="@color/yellow"
                android:onClick="cancel"
                android:text="Cancel"
                android:textColor="#FFF"
                tools:ignore="OnClick" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:layout_marginLeft="9dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="9dp"
            android:background="@drawable/list"
            android:baselineAligned="false"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="19dp"
                android:textAlignment="center"
                android:text="Recieved"
                android:layout_marginRight="19dp"
                android:textColor="#FF725E"
                android:textSize="15dp"
                android:textStyle="italic"
                tools:ignore="DuplicateIds"
                android:gravity="center_horizontal" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="19dp"
                android:textAlignment="center"
                android:text="Shop Parts"
                android:layout_marginRight="19dp"
                android:textColor="@color/b"
                android:textSize="22dp"
                android:textStyle="italic"
                tools:ignore="DuplicateIds"
                android:gravity="center_horizontal" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="19dp"
                android:layout_marginRight="19dp"
                android:text="VMAX RM200"
                android:textColor="#000"
                android:textAlignment="center"
                android:textSize="15dp"
                android:textStyle="bold"
                tools:ignore="DuplicateIds"
                android:gravity="center_horizontal" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="19dp"
                android:layout_marginRight="19dp"
                android:layout_marginTop="10dp"
                android:text="Continental Tires"
                android:textColor="#000"
                android:textAlignment="center"
                android:textSize="15dp"
                android:textStyle="bold"
                tools:ignore="DuplicateIds"
                android:gravity="center_horizontal" />

        </LinearLayout>



    </LinearLayout>
    <LinearLayout
        android:id="@+id/paid"
        android:layout_width="match_parent"
        android:layout_height="73dp"
        android:layout_marginTop="5dp"
        android:background="@color/b"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="19dp"
            android:layout_marginRight="19dp"
            android:text="Back to Home"
            android:textColor="#FFF"
            android:textAlignment="center"
            android:textSize="22dp"
            android:textStyle="italic"
            tools:ignore="DuplicateIds"
            android:gravity="center_horizontal" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>